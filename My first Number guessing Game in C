#include<studio.h>
#include<stdlib.h>
#include<time.h>

int main () {
int number,guess,nguesses =1;
srand(time(0));
number=rand()%100+1;
do{
Printf("guess the number between 1to 100\n");
}
else if (guess <number){
Printf (" higher number please!\n");
}
else
{
Printf("You guessed it in %d attempts \n", nguesses);
}
nguesses++;
} while (guess!=number);
return 0;
}
